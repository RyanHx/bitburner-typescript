{
  "name": "bitburner-typescript-template",
  "version": "0.0.0",
  "scripts": {
    "watch": "concurrently \"npx bitburner-filesync@latest\" \"npx tsc -w\"",
    "filesync": "npx bitburner-filesync@latest",
    "filesync:f": "npx bitburner-filesync@latest --pushAllOnConnection",
    "filesync:d": "npx bitburner-filesync@latest --dry",
    "ts:watch": "npx tsc -w"
  },
  "author": "hydroflame, based on work by SlyCedix",
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "bitburner-filesync": "^1.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "typescript": "^4.7.4"
  }
}